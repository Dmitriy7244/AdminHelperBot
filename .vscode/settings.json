{
  "deno.enable": true,
  "deno.unstable": true,
  "files.exclude": {
    "**/__pycache__": true,
    "generated": true,
    "node_modules": true,
    "generated\\client": true,
    "poster": true
  },
  "workspace.isHidden": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
