{
  "deno.enable": true,
  "deno.unstable": true,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": false,
    "**/.hg": false,
    "**/CVS": false,
    "**/.DS_Store": false,
    "**/Thumbs.db": false,
    "**/__pycache__": true,
    "generated": true,
    "node_modules": true,
    "generated\\client": true,
    "poster": true
  },
  "workspace.isHidden": true,
  "editor.defaultFormatter": "denoland.vscode-deno",
  "[typescript]": {
    "editor.defaultFormatter": "denoland.vscode-deno"
  }
}
